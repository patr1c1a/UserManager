{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=UserManager.db"
  },
  "JwtSettings": {
    "Issuer": "UserManagerAPI",
    "Audience": "UserManagerClients",
    "ExpirationMinutes": 60
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "UserManager": "Debug"
    }
  },
  "AllowedHosts": "*"
}